                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH AllCombinations AS (
    SELECT DISTINCT FORMAT(thoigianluutru, 'yyyyMMdd') AS maTG, maMH, maCH
    FROM MatHangDuocLuuTru
    UNION
    SELECT DISTINCT FORMAT(thoigiandat, 'yyyyMMdd') AS maTG, maMH, maCH
    FROM MatHangDuocDat
),
TongNhap AS (
    SELECT 
        a.maTG,
        a.maMH,
        a.maCH,
        SUM(m.soluongtrongkho) AS tongnhap
    FROM AllCombinations a
    JOIN MatHangDuocLuuTru m 
        ON m.maMH = a.maMH AND m.maCH = a.maCH 
        AND FORMAT(m.thoigianluutru, 'yyyyMMdd') &lt;= a.maTG
    GROUP BY a.maTG, a.maMH, a.maCH
),
TongBan AS (
    SELECT 
        a.maTG,
        a.maMH,
        a.maCH,
        SUM(m.soluongdat) AS tongban
    FROM AllCombinations a
    JOIN MatHangDuocDat m 
        ON m.maMH = a.maMH AND m.maCH = a.maCH 
        AND FORMAT(m.thoigiandat, 'yyyyMMdd') &lt;= a.maTG
    GROUP BY a.maTG, a.maMH, a.maCH
)
SELECT 
    a.maTG,
    a.maMH,
    a.maCH,
    ISNULL(n.tongnhap, 0) - ISNULL(b.tongban, 0) AS soluongconlaitrongkho
FROM AllCombinations a
LEFT JOIN TongNhap n 
    ON a.maTG = n.maTG AND a.maMH = n.maMH AND a.maCH = n.maCH
LEFT JOIN TongBan b 
    ON a.maTG = b.maTG AND a.maMH = b.maMH AND a.maCH = b.maCH
ORDER BY a.maCH, a.maMH, a.maTG;</property>
