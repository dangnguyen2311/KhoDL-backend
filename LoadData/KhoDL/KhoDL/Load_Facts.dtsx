                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH AllDatesAndItems AS (
    SELECT DISTINCT FORMAT(thoigianluutru, 'yyyyMMdd') AS maTG, maMH
    FROM MatHangDuocLuuTru
    UNION
    SELECT DISTINCT FORMAT(thoigiandat, 'yyyyMMdd') AS maTG, maMH
    FROM MatHangDuocDat
),
TongNhap AS (
    SELECT 
        d.maTG,
        d.maMH,
        SUM(mh.soluongtrongkho) AS tongnhap
    FROM AllDatesAndItems d
    JOIN MatHangDuocLuuTru mh 
        ON mh.maMH = d.maMH 
        AND FORMAT(mh.thoigianluutru, 'yyyyMMdd') &lt;= d.maTG
    GROUP BY d.maTG, d.maMH
),
TongBan AS (
    SELECT 
        d.maTG,
        d.maMH,
        SUM(md.soluongdat) AS tongban
    FROM AllDatesAndItems d
    JOIN MatHangDuocDat md 
        ON md.maMH = d.maMH 
        AND FORMAT(md.thoigiandat, 'yyyyMMdd') &lt;= d.maTG
    GROUP BY d.maTG, d.maMH
)
SELECT 
    d.maTG,
    d.maMH,
    ISNULL(n.tongnhap, 0) - ISNULL(b.tongban, 0) AS soluongconlai
FROM AllDatesAndItems d
LEFT JOIN TongNhap n ON d.maTG = n.maTG AND d.maMH = n.maMH
LEFT JOIN TongBan b ON d.maTG = b.maTG AND d.maMH = b.maMH
ORDER BY d.maMH, d.maTG;</property>
