    nh.maMH,
    SUM(nh.soluongtrongkho) - ISNULL(SUM(xuat.soluongban), 0) AS soluongconlaitrongkho
FROM MatHangDuocLuuTru nh
LEFT JOIN (
    SELECT
        ch.maCH,
        mdd.maMH,
        SUM(mdd.soluongdat) AS soluongban
    FROM MatHangDuocDat mdd
    JOIN DonDatHang ddh ON mdd.maDon = ddh.maDon
    JOIN KhachHang kh ON ddh.maKH = kh.maKH
    JOIN CuaHang ch ON kh.maTP = ch.maTP -- Khớp cửa hàng cùng thành phố với khách hàng
    GROUP BY ch.maCH, mdd.maMH
) xuat ON nh.maCH = xuat.maCH AND nh.maMH = xuat.maMH
GROUP BY nh.maCH, nh.maMH</property>
