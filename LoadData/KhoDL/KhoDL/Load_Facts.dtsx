                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Use BTL_IDB;

SELECT 
    CONVERT(VARCHAR(8), thoigiandat, 112) AS maTG, 
    DAY(thoigiandat) AS ngay, 
    MONTH(thoigiandat) AS thang, 
    CASE 
        WHEN MONTH(thoigiandat) BETWEEN 1 AND 3 THEN 1 
        WHEN MONTH(thoigiandat) BETWEEN 4 AND 6 THEN 2 
        WHEN MONTH(thoigiandat) BETWEEN 7 AND 9 THEN 3 
        WHEN MONTH(thoigiandat) BETWEEN 10 AND 12 THEN 4 
    END AS quy, 
    YEAR(thoigiandat) AS nam
FROM MatHangDuocDat
GROUP BY 
    CONVERT(VARCHAR(8), thoigiandat, 112), 
    DAY(thoigiandat), 
    MONTH(thoigiandat), 
    YEAR(thoigiandat)

UNION 

SELECT 
    CONVERT(VARCHAR(8), thoigianluutru, 112) AS maTG, 
    DAY(thoigianluutru) AS ngay, 
    MONTH(thoigianluutru) AS thang, 
    CASE 
        WHEN MONTH(thoigianluutru) BETWEEN 1 AND 3 THEN 1 
        WHEN MONTH(thoigianluutru) BETWEEN 4 AND 6 THEN 2 
        WHEN MONTH(thoigianluutru) BETWEEN 7 AND 9 THEN 3 
        WHEN MONTH(thoigianluutru) BETWEEN 10 AND 12 THEN 4 
    END AS quy, 
    YEAR(thoigianluutru) AS nam
FROM MatHangDuocLuuTru
GROUP BY 
    CONVERT(VARCHAR(8), thoigianluutru, 112), 
    DAY(thoigianluutru), 
    MONTH(thoigianluutru), 
    YEAR(thoigianluutru)

UNION

SELECT 
    CONVERT(VARCHAR(8), ngaydathang, 112) AS maTG, 
    DAY(ngaydathang) AS ngay, 
    MONTH(ngaydathang) AS thang, 
    CASE 
        WHEN MONTH(ngaydathang) BETWEEN 1 AND 3 THEN 1 
        WHEN MONTH(ngaydathang) BETWEEN 4 AND 6 THEN 2 
        WHEN MONTH(ngaydathang) BETWEEN 7 AND 9 THEN 3 
        WHEN MONTH(ngaydathang) BETWEEN 10 AND 12 THEN 4 
    END AS quy, 
    YEAR(ngaydathang) AS nam
FROM DonDatHang
GROUP BY 
    CONVERT(VARCHAR(8), ngaydathang, 112), 
    DAY(ngaydathang), 
    MONTH(ngaydathang), 
    YEAR(ngaydathang);</property>
